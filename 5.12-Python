print (d)

# ライブラリの関数を使って、一通りの計算をした

data = np.arange(1,7)
print (data)
data2 = data.reshape(2,3)
print (data2)
print(np.sum(data2))
print(np.sum(data2, axis = 0))
print(np.sum(data2, axis = 1))
print(np.mean(data2))
print(np.median(data2))
print(np.std(data2))
print(np.var(data2))
print(np.max(data2))
print(np.min(data2))
print(np.argmax(data2))
print(np.argmin(data2))
